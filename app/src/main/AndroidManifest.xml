<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.zaneta.igandroidapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/log"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ActivityFR"
            android:label="@string/title_activity_fr"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ActivityGER"
            android:label="@string/title_activity_ger"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ActivityUK"
            android:label="@string/title_activity_uk"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>